{
  "name": "oil-painting-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "export": "next export",
    "type-check": "tsc --noEmit",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "build:production": "npm run prisma:generate && npm run build && npm run export",
    "build:beta": "npm run prisma:generate && npm run build && npm run export",
    "ci:test": "npm run lint:check && npm run type-check && npm run test",
    "firebase:deploy": "firebase deploy --only hosting:production",
    "firebase:deploy:beta": "firebase deploy --only hosting:beta",
    "firebase:deploy:preview": "firebase hosting:channel:deploy preview --expires 7d",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.14.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "canvas": "^3.1.2",
    "firebase": "^12.1.0",
    "form-data": "^4.0.4",
    "lucide-react": "^0.379.0",
    "next": "14.2.3",
    "next-auth": "^4.24.11",
    "prisma": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "playwright": "^1.54.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  }
}
